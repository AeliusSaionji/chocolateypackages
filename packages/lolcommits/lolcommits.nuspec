<?xml version="1.0" encoding="utf-8"?>
<package xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <metadata>
    <id>lolcommits</id>
    <title>lolcommits</title>
    <version>0.5.4</version>
    <authors>Matthew Rothenberg</authors>
    <owners>Anthony Mastrean</owners>
    <summary>lolcommits takes a snapshot with your webcam every time you git commit code.</summary>
    <description>lolcommits takes a snapshot with your webcam every time you git commit code, and archives a lolcat style image with it. It's selfies for software developers. git blame has never been so much fun.</description>
    <projectUrl>http://mroth.github.io/lolcommits/</projectUrl>
    <tags></tags>
    <licenseUrl>https://github.com/mroth/lolcommits/blob/master/LICENSE</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <iconUrl>http://anthonymastrean.github.io/chocolateypackages/icons/lolcommits.png</iconUrl>
    <dependencies>
      <dependency id="ruby" version="[1.9.3, 2.0)" />
      <dependency id="ruby.devkit" /> 
      <dependency id="imagemagick" version="[6.7.9, 6.7.10)" /> 
    </dependencies>
  </metadata>
</package>
